{
  "name": "ts-cli-grpc-plugin-workspace",
  "version": "0.0.1",
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "knip": "^5.63.1",
    "lerna": "^8.2.3",
    "prettier": "^3.6.2",
    "syncpack": "14.0.0-alpha.19",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.43.0"
  },
  "packageManager": "pnpm@10.15.0",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "cleanbuild": "lerna run clean && lerna run build",
    "fix": "pnpm fix:all",
    "fix:all": "pnpm fix:packages && pnpm fix:knip && pnpm fix:syncpack",
    "fix:format": "pnpm fix:format:prettier && pnpm fix:format:syncpack",
    "fix:format:prettier": "prettier --write .",
    "fix:format:syncpack": "syncpack format",
    "fix:knip": "knip --fix --format",
    "fix:packages": "lerna run fix",
    "fix:syncpack": "syncpack fix",
    "lint": "pnpm lint:all",
    "lint:all": "pnpm lint:packages && pnpm lint:knip && pnpm lint:syncpack",
    "lint:knip": "knip",
    "lint:packages": "lerna run lint:all",
    "lint:syncpack": "syncpack lint",
    "test": "lerna run test",
    "prepare": "husky",
    "commitlint": "commitlint --from=HEAD~1 --to=HEAD --verbose"
  },
  "workspaces": [
    "ts-cli-grpc-plugin",
    "examples/*"
  ]
}
